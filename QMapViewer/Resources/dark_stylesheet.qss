/*----------------------------------------------------------------------------*/
QLabel, QCheckBox, QRadioButton{
    color: rgb(220, 220, 220);


}

QLabel[alive="true"] {

   background-color:qlineargradient(x1:0, y1:1, x2:0, y2:0, stop:0 rgb(20,20,20), stop:0.5 rgb(40,40,40), stop:1 rgb(10,10,10)); 


    color: rgb(220,220,220);

    border:1px solid black;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;

    min-width:60px;

    border: 1px solid black;
    border-top: 1px solid rgb(100,100,100);
    border-right: 1px solid rgb(100,100,100);

    border-left:5px solid rgb(30,144,255);

}

QLabel[alive="false"] {

   background-color:qlineargradient(x1:0, y1:1, x2:0, y2:0, stop:0 rgb(20,20,20), stop:0.5 rgb(40,40,40), stop:1 rgb(10,10,10)); 
/*
    background-color:qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 rgb(20,20,20), stop:1 rgb(40,40,40));*/

    color: rgb(220,220,220);

    border:1px solid rgb(100,100,100);
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;

    min-width:60px;

    border: 1px solid black;
    border-top: 1px solid rgb(100,100,100);
    border-right: 1px solid rgb(100,100,100);

    border-left:5px solid rgb(255,40,40);

}
